package  {		import flash.utils.Dictionary;	import flash.utils.ByteArray;		public class DictionaryJSON {				/*				This supplies a method to do json stringification of dictionaries				*/				static public function fixJSON() : void {						Dictionary.prototype.toJSON = function(k:String): * {								var json : String = "{";								var key : String;								for (key in this) {										json += JSON.stringify(key) + ": " + JSON.stringify(this[key]) + ",\n" ;									}								json += "}";								// it string wraps any returned strings, fuck.				// what about if I just call it and write to file. easy				// or better yet if the top level container is a dict,				// then real = jsondecode(decoded[0])								return json;							};					}			}	}